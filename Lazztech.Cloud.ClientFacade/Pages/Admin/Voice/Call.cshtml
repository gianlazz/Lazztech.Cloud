@page
@model Lazztech.Cloud.ClientFacade.Pages.Admin.Voice.CallModel
@{
    ViewData["Title"] = "Call";
}

<h2>Call</h2>

<form method="post">
    <div class="form-group">
        <label asp-for="PhoneNumber" class="custom-control-label"></label>
        <input asp-for="PhoneNumber" class="form-control" />
        <span asp-validation-for="PhoneNumber" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Call" class="btn btn-default" />
    </div>
</form>
